    "sass-loader": "^4.0.0",
    "shelljs": "^0.7.0",
    "style-loader": "^0.13.0",
    "tinymce": "^4.8.0",
    "ts-helpers": "^1.1.1",
    "ts-loader": "^0.8.1",
    "tslint": "^3.4.0",
    "tslint-loader": "^2.1.0",
    "typedoc": "^0.4.4",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.13",
    "webpack-dashboard": "^0.1.8",
    "webpack-dev-server": "^1.14.1",
    "webpack-node-externals": "^1.3.3",
    "zone.js": "^0.6.12"
  },
  "typings": "./dist/src/index.d.ts",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ]
